{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "del-cli --force cypress/reports",
    "doc": "jsdoc -c jsdoc.json",
    "pretest": "npm run clean:reports",
    "combine-reports": "npx mochawesome-merge cypress/reports/mocha/*.json -o cypress/reports/mochaCombined/report.json",
    "generate-report": "npx marge cypress/reports/mochaCombined/report.json -f report -o cypress/reports/mochaHtml --inline",
    "posttest": "npm run combine-reports && npm run generate-report",
    "all": "(cypress run --group run_${BUILD_ID} --ci-build-id ${BUILD_ID} --parallel --record --headless --browser chrome)",
    "check": "(cypress run --group run_${BUILD_ID} --ci-build-id ${BUILD_ID} --parallel --record --headless --browser chrome --config ignoreTestFiles= '' --spec cypress/integration/com.faceit.test/ignoredTestFiles/Test.js)",
    "custom": "(cypress run --group run_${BUILD_ID} --ci-build-id ${BUILD_ID} --parallel --record --headless --browser chrome --spec cypress/integration/com.faceit.test/Custom.js)",
    "core": "(cypress run --group run_${BUILD_ID} --ci-build-id ${BUILD_ID} --parallel --record --headless --browser chrome --spec cypress/integration/com.faceit.test/TeamCore.js)",
    "follo": "npm run pretest && (npx cypress run --spec cypress/integration/com.faceit.test/Custom.js --headless --browser chrome || (npm run posttest && exit 1))"
  },

  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^6.2.1",
    "cypress-iframe": "1.0.x",
    "cypress-multi-reporters": "1.4.x",
    "cypress-xpath": "1.6.x",
    "del-cli": "3.0.x",
    "ini": ">=1.3.6",
    "jsdoc": "^3.6.6",
    "mocha": "8.2.x",
    "mocha-junit-reporter": "2.0.x",
    "mochawesome": "6.2.x",
    "mochawesome-merge": "4.2.x",
    "typescript": "4.0.x"
  },

  "dependencies": {
    "buffer": "^6.0.2",
    "cypress-commands": "^1.1.0"
  }
}
